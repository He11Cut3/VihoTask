@model IEnumerable<Chat>
<ul class="list custom-scrollbar">
    <li class="clearfix">
        <div class="media">
            <div class="chat-body">
                @foreach (var chat in Model)
                {
                    <a class="room-button" asp-controller="Chat" asp-action="Chat" asp-route-id="@chat.Id">
                        @chat.Name
                    </a>
                }
            </div>
        </div>
    </li>
</ul>